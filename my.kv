Screenmanager:
    MainScreen:
    SettingScreen:
    

<SettingScreen>:
    name: 'setting'
    Button:
        size_hint: 0.1, 0.1
        pos: root.width * 0.9, root.height * 0.9
        text: 'Back'
        on_press: 
            app.root.current = 'main'
            root.manager.transition.direction = 'right'

<MainScreen>:
    name: 'main'
    BoxLayout:
        orientation:'vertical'
        size_hint: 0.3, 0.5
        spacing: 20
        padding: 40
        Button:
            text: 'Continue'
        Button:
            text: 'Newgame'
        Button:
            text: 'Setting'
            on_press:
                app.root.current = 'setting'
                root.manager.transition.direction = 'left'
        Button:
            text: 'Exit'
            on_press: root.close_app()
            background_normal: ''
            background_color: (238/255, 250/255, 0/255, 1)